//converting coins to dollars
#include <iostream>
#include <iomanip>
using namespace std;

int main() {
   double dollars;
   int quarters;
   int dimes;
   int nickels;
   int pennies;
   
   /* Type your code here. */
   cin >> quarters >> dimes >> nickels >> pennies;
   
   cout << fixed << setprecision(2);
   
   dollars = (quarters * 0.25) + (dimes * 0.10) + (nickels * .05) + (pennies * .01);
   cout << "Amount: $" << dollars << endl;
   
   return 0;
}
