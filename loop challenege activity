/*
Integer numVals is read from input representing the number of integers to be read next. Use a loop to read the remaining integers from input. For each integer read in the loop, output the integer followed by " bees.". End each output with a newline.

Ex: If the input is:

4
75 70 65 90
then the output is:

75 bees.
70 bees.
65 bees.
90 bees.
*/


#include <iostream>
using namespace std;

int main() {
   int numVals;
   int value;
   int i;

   cin >> numVals;

   for (i = 0; i < numVals; i++) {
      cin >> value;
      cout << value << " bees." << endl;
   }

   return 0;
}
